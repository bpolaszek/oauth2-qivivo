{
    "name": "bentools/oauth2-qivivo",
    "description": "Qivivo OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Beno!t POLASZEK",
            "email": "bpolaszek@gmail.com",
            "homepage": "https://github.com/bpolaszek"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authorisation",
        "qivivo"
    ],
    "require": {
        "php": ">=5.5.0",
        "league/oauth2-client": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "BenTools\\Qivivo\\OAuth2\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BenTools\\Qivivo\\Tests\\OAuth2\\Provider\\": "tests/src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
